<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>EmployeeServlet</servlet-name>
    <servlet-class>servlet.EmployeeServlet</servlet-class>
  </servlet>
 
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>seleDpServlet</servlet-name>
    <servlet-class>servlet.seleDpServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>selecDpServlet</servlet-name>
    <servlet-class>servlet.selecDpServlet</servlet-class>
  </servlet>



  <servlet-mapping>
    <servlet-name>EmployeeServlet</servlet-name>
    <url-pattern>/EmployeeServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>seleDpServlet</servlet-name>
    <url-pattern>/seleDpServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>selecDpServlet</servlet-name>
    <url-pattern>/selecDpServlet</url-pattern>
  </servlet-mapping>
</web-app>